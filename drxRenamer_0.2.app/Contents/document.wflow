				<dict>
					<key>COMMAND_STRING</key>
					<string>#!/bin/bash
#Written by Alex Golding 2018



getName()
{	
	#echo in getName
	drxFile="$1"
	#echo drxFile is $drxFile
	reelName=$(sed -n -e 's/&lt;ReelName&gt;//' -e 's/&lt;\/ReelName&gt;//p' "$drxFile")
	#echo Reelname $reelName
	srcTc=$(sed -n -e 's/&lt;SrcTC&gt;//' -e 's/:/-/' -e 's/:/-/' -e 's/:/-/' -e 's/:/-/' -e 's/&lt;\/SrcTC&gt;//p' "$drxFile")
	srcTc="$(echo -e "${srcTc}" | sed -e 's/^[[:space:]]*//')"		#removes space at beginning of srcTc
	newName="$reelName"_"$srcTc"
	echo $newName
}

renameDrx()
{
	drxFile="$1"
	oldName=$(basename "${drxFile}")
	oldName=${oldName%%".drx"}
	echo 
	newName=$(getName "$drxFile")
	echo $oldName will be replaced by $newName
	for file in "$workingDir"/$oldName.*
	do
		echo found $file
		extension=${file##"$workingDir/$oldName."}
		#echo has extension $extension
		echo mv "$file" "$workingDir/$newName.$extension"
		mv "$file" "$workingDir/$newName.$extension"		
	done	
}

renameFolder()
{
echo this will rename all files matching the name:
for file in "$workingDir"/*.drx
do
	echo ${file%%".drx"}
done
for file in "$workingDir"/*.drx
do
	echo reading $file	
	renameDrx "$file"
	echo
done
echo done
}

echo
if [ -d "$1" ]; then
	workingDir="${1%%/}"							#remove the trailing /
	echo $workingDir
	renameFolder "$workingDir"
	exit
elif [ -f "$1" ]; then
	fileName=$1
	echo filename is $fileName
	workingDir=$(dirname "${fileName}")
	extension="${fileName##*.}"
	echo $extension
	echo from dir $workingDir
	if [ "$extension" != "drx" ]; then
		echo "not a drx file"
		exit
	fi
	echo file is "$fileName"
	renameDrx "$1"
	exit
else 
	echo unknown
	exit
fi
	




if [ -d "$2" ]; then
outputDir="$2"
else
outputDir="$workingDir"
fi

</string>
